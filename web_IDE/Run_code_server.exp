#!/usr/bin/expect

set timeout -1
set user_name [lindex $argv 0]

spawn su $user_name
expect "\\\$"
send "cd /tmp/Install_code_server\r"
expect "\\\$"

# run code server
send "nohup code-server > /home/$user_name/log_file/code_server.log 2>&1 &\r"
expect "\\\$"
send "exit\r"
expect "*"
send "exit\r"
expect eof
